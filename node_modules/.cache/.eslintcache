                                    <table className=\"table\">\r\n                                        <tbody>\r\n                                                <tr>\r\n                                                    <th >Item Total Amount:</th>\r\n                                                    <td>\r\n                                                        { \r\n                                                            invoiceDetails?.length ? (\r\n                                                                <div className=\"col-5\">\r\n                                                                    <input type=\"text\" name=\"itemTotal\" value={invoice?.itemTotal} onChange={(e) => setInvoiceFinalAmount(e.target.value)} />&nbsp;&nbsp;&nbsp;Rs.\r\n                                                                </div>\r\n                                                            ) : (0)\r\n                                                        }\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <th >Item Tax Amount:</th>\r\n                                                    <td>\r\n                                                        { \r\n                                                            invoiceDetails?.length ? (\r\n                                                                <div className=\"col-5\">\r\n                                                                    <input type=\"text\" name=\"itemTax\" value={invoice?.itemTax} onChange={(e) => setInvoiceFinalAmount(e.target.value)} />&nbsp;&nbsp;&nbsp;Rs.\r\n                                                                </div>\r\n                                                            ) : (0)\r\n                                                        }\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <th> Billled Amount: </th>\r\n                                                    <td>\r\n                                                        { \r\n                                                            invoiceDetails?.length ? (\r\n                                                                <div className=\"col-5\">\r\n                                                                    <input type=\"text\" name=\"billTotal\" value={invoice?.billTotal} onChange={(e) => setInvoiceFinalAmount(e.target.value)} />&nbsp;&nbsp;&nbsp;Rs.\r\n                                                                </div>\r\n                                                            ) : (0)\r\n                                                        }\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <th> Round Value : </th>\r\n                                                    <td>\r\n                                                        { \r\n                                                            invoiceDetails?.length ? (\r\n                                                                <div className=\"col-5\">\r\n                                                                    <input type=\"text\" name=\"roundValue\" value={invoice?.roundValue} />&nbsp;&nbsp;&nbsp;Rs.\r\n                                                                </div>\r\n                                                            ) : (0)\r\n                                                        }\r\n                                                    </td>\r\n                                                </tr>\r\n